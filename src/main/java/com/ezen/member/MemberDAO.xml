<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

	<mapper namespace="com.ezen.member.MemberService">
<insert id="insert">
INSERT INTO T_MEMBER VALUES(#{param1},#{param2},#{param3},#{param4},#{param5},#{param6},#{param7})

</insert>
	
<select id="output" resultType="com.ezen.member.TmemberVO">
	SELECT * FROM T_MEMBER 
</select>	

   <select id="login"  resultType="com.ezen.member.TmemberVO"    >
	
		SELECT * FROM T_MEMBER	
		WHERE id=#{param1} and pw=#{param2}		
	
  </select>
  
  
</mapper>
